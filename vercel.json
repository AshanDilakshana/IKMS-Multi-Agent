{
    "devCommand": "uvicorn src.app.api:app --host 0.0.0.0 --port 8000 --reload",
    "builds": [
        {
            "src": "src/app/api.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app/api.py"
        }
    ]
}